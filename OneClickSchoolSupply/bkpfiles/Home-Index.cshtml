@{
    ViewBag.Title = "Home Page";
}

<script src="/scripts/jquery.min.js"></script>
<script src="/scripts/bootstrap.min.js"></script>

<script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
<!-- include Cycle2 
  <script type='text/javascript'  src='http://jqueryjs.googlecode.com/files/jquery-1.3.2.min.js'></script>
    <script src="http://malsup.github.com/jquery.cycle2.js"></script> or downloaded version below.
    -->
<script src="/scripts/jquery.cycle2.min.js"></script>

<script type="text/javascript">

    $(document).ready(function () {
        $(".btn-info").tooltip({ placement: 'top' });
        
    });
    </script>

<script type="text/javascript">
    $(document).ready(function () {
        $(".img-circle").hover(function () {

            $(this).css("cursor", "pointer");
            $(this).animate({ width: ["toggle", "swing"], height: ["toggle", "swing"], opacity: "toggle" }, 'slow');
        });

        $('#imgSmile').mouseleave(function () {
            $(this).animate({ width: "150px" }, { height: "150px" }, 'slow');
        });
    });
</script>



    @section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">

                <h2>@ViewBag.Message</h2>
            </hgroup>
            <div class="cycle-slideshow"
    data-cycle-loader="wait"
    data-cycle-fx="scrollHorz"
    data-cycle-pause-on-hover="true"
    data-cycle-speed="175">
   <!-- empty element for caption -->
   <div class="cycle-caption"></div>
   <img src="/Images/QualityProducts.jpg" width="870" height="250">
   <img src="/Images/WeDonate.jpg" width="870" height="250">
   <img src="/Images/PrepackageSchoolSupply.jpg" width="870" height="250">
   <img src="/Images/MadeInUSA.jpg" width="870" height="250">
   <img src="/Images/FreeShipping.jpg" width="870" height="250">
   <img src="/Images/CompareUs.jpg" width="870" height="250">

</div>
</div>
</section>
       }


<div class="row">
<div class="col-md-4">
<center>
    <img width="150" height="150" src="Images/how-it-works.jpg" class="img-circle" id="imgsmile" />
<h5 class="text-info">How it Works!</h5>
    <li class="btn btn-info" data-toggle=" tooltip" data-original-title="Find out more">@Html.ActionLink("Read More >>", "HowItWorks", "Home")</li>
  
</center>
</div>
<div class="col-md-4">
<center>
<img width="150" height="150" src="Images/buy1.png" class="img-circle" />
<h5 class="text-info">Shop By List!</h5>
    <li class="btn btn-info" data-toggle=" tooltip" data-original-title="Find out more">@Html.ActionLink("Read More >>", "HowToBuy", "Home")</li>
</center>
</div>
<div class="col-md-4">
<center>
<img width="150" height="150" src="Images/ContactUs.jpg" class="img-circle" />
<h5 class="text-info">Contact Us!</h5>
    <li class="btn btn-info" data-toggle=" tooltip" data-original-title="Find out more">@Html.ActionLink("Read More >>", "Contact", "Home")</li>
</center>

</div>

</div>
